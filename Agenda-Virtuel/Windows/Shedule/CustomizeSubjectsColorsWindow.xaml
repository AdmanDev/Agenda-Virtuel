<Window x:Class="Agenda_Virtuel.Windows.Shedule.CustomizeSubjectsColorsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:agenda="clr-namespace:Agenda_Virtuel"
        xmlns:local="clr-namespace:Agenda_Virtuel.Windows.Shedule"
        mc:Ignorable="d"
        x:ClassModifier="internal" Title=""  Width="400" Height="472" Style="{DynamicResource WindowsStyle}"  WindowStartupLocation="CenterScreen">
    <StackPanel Orientation="Vertical">
        <agenda:WindowHeader/>

        <GroupBox Header="Couleurs">
            <ScrollViewer VerticalScrollBarVisibility="Auto" Height="404">
                <StackPanel x:Name="SP_Colors" Orientation="Vertical"/>
            </ScrollViewer>
        </GroupBox>
    </StackPanel>
</Window>
